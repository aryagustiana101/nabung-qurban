{
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e .env -- prisma migrate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:seed": "dotenv -e .env -- prisma db seed"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@prisma/client": "^6.9.0",
    "@repo/common": "workspace:*",
    "@t3-oss/env-core": "^0.13.8",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "zod": "^3.25.62"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^24.0.0",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.9.0",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3"
  }
}
